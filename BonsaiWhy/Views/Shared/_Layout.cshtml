<!doctype html>
<html lang='en'>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta property="og:image" content='/Content/png/graph.png'>
    <meta property="og:image:width" content="400">
    <meta property="og:image:height" content="400">

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i" rel="stylesheet">
    @RenderSection("css", required: false)
</head>
<body>
    <header id="body-header" class="header">
        @Html.Partial("_Header")
    </header>
    <main id="body-main" class="main">
        @RenderBody()
    </main>
    <footer id="body-footer" class="footer">
        @Html.Partial("_Footer")
        <ins class="adsbygoogle"
             style="display:block"
             data-ad-client="ca-pub-7496598263316713"
             data-ad-slot="8994102982"
             data-ad-format="auto">
        </ins>
    </footer>
    <script src="~/Scripts/dist/shared/navbar.js"></script>
    @RenderSection("scripts", required: false)
    <script>
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
		ga('create', 'UA-73068722-1', 'auto');
		ga('send', 'pageview');
    </script>
        <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
        <script>(adsbygoogle = window.adsbygoogle || []).push({});</script>
</body>
</html>
